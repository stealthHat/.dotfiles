# To make .kube go to .config
export KUBECONFIG=$HOME/.config
# To make .gnupg go to .config
export GNUPGHOME=$HOME/.config
# To make .oh-my-zsh go to .config
export ZDOTDIR=$HOME/.config/zsh
# To make .zplug go to .config 
export ZPLUG_HOME=$HOME/.config/zsh/.zplug



# i3wm variables
export BROWSER="brave"
export EDITOR="nvim"
